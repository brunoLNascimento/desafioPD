CREATE TABLE `colecao_tb` (
	`ID_COLECAO` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`NOME_COLECAO` VARCHAR(100) NULL DEFAULT NULL,
	`DATA_CRIACAO` VARCHAR(10) NULL DEFAULT NULL,
	`DATA_EXCLUSAO` VARCHAR(10) NULL DEFAULT NULL,
	`ATIVO` VARCHAR(1) NOT NULL DEFAULT '1',
	INDEX `Index 1` (`ID_COLECAO`)
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=1
;


CREATE TABLE `disco_tb` (
	`ID_DISCO` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`ID_COLECAO` BIGINT(20) NOT NULL,
	`NOME_DISCO` VARCHAR(100) NOT NULL,
	`DESC_DISCO` VARCHAR(400) NULL DEFAULT NULL,
	`DATA_LANCAMENTO` VARCHAR(10) NULL DEFAULT NULL,
	`ATIVO` CHAR(1) NOT NULL DEFAULT '1',
	PRIMARY KEY (`ID_DISCO`),
	INDEX `FK__colecao_tb` (`ID_COLECAO`),
	CONSTRAINT `FK__colecao_tb` FOREIGN KEY (`ID_COLECAO`) REFERENCES `colecao_tb` (`ID_COLECAO`)
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=1
;
